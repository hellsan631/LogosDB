{
	"name": "hellsan631/logosdb",
	"description": "A database micro-framework for creating simple DB interaction without the need for a full framework",
	"type": "library",
	"keywords": ["framework"],
	"homepage": "http://mathew-kleppin.com/blog/",
	"license": "MIT",
	"authors": [
		{
			"name": "Mathew Kleppin",
			"homepage": "http://mathew-kleppin.com/blog/"
		}
	],
	"require": {
		"php": ">=5.5",
		"ext-mcrypt": "*"
	},
    "autoload": {
        "classmap":[
            "lib/Testing"
        ],
        "psr-4": {
            "Logos\\": "lib/",
            "Logos\\DB\\": "lib/Logos/DB/",
            "Logos\\DB\\Mongo\\": "lib/Logos/DB/Mongo",
            "Logos\\DB\\MySQL\\": "lib/Logos/DB/MySQL",
            "Logos\\Resources\\": "lib/Logos/Resources/",
            "Logos\\Security\\": "lib/Logos/Security/"

        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hellsan631/LogosDB"
        }
    ]
}